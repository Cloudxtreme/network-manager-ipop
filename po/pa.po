#: ../properties/auth-helpers.c:96
msgid "Choose your private key..."
msgstr "ਆਪਣੀ ਪ੍ਰਾਈਵੇਟ ਕੁੰਜੀ ਚੁਣੋ..."

#: ../properties/auth-helpers.c:256
msgid "PEM certificates (*.pem, *.crt, *.key)"
